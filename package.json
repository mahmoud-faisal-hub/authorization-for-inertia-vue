    }
  },
  "peerDependencies": {
    "vue": "3.x"
  },
  "devDependencies": {
    "@inertiajs/vue3": "^1.0.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "vite": "^4.2.1"
  },
  "license": "MIT"
}
