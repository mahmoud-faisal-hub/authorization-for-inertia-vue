{
  "name": "authorization-for-laravel-inertia-vue",
  "version": "1.1.1",
  "author": "Mahmoud Faisal <dev.mahmoudfaisal@gmail.com>",
  "description": "Authorization package for roles and permissions with super roles to use in Vue.js with Inertia.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mahmoud-faisal-hub/authorization-for-laravel-inertia-vue.git"
  },
  "bugs": {
    "url": "https://github.com/mahmoud-faisal-hub/authorization-for-laravel-inertia-vue/issues"
  },
  "keywords": [
    "vue",
    "vue3",
    "authorization-for-laravel-inertia-vue",
    "authorization-checks",
    "authorization",
    "permissions",
    "roles",
    "inertia",
    "laravel",
    "package"
  ],
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/authorization-for-laravel-inertia-vue.umd.js",
  "module": "./dist/authorization-for-laravel-inertia-vue.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/authorization-for-laravel-inertia-vue.es.js",
      "require": "./dist/authorization-for-laravel-inertia-vue.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "semantic-release": "semantic-release --branches main"
  },
  "peerDependencies": {
    "vue": "3.x"
  },
  "dependencies": {
    "@inertiajs/vue3": "^1.0.2"
  },
  "devDependencies": {
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/tsconfig": "^0.1.3",
    "cz-conventional-changelog": "^3.3.0",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-typescript2": "^0.34.1",
    "semantic-release": "^21.0.0",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vue-tsc": "^1.2.0"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
